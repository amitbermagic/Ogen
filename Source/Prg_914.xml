        <SQL_FORM DB="DB_AGUDA" RESTAB="Memory">
          <SQL_STMT_U val="select H.NUMERATOR,LAKOH_TYPE,TZ_TYPE,&#xD;&#xA;case  &#xD;&#xA;when LAKOH_TYPE=2 and TZ_TYPE=5 and TT.KVUTZAT_MASTER in (8,12) then 1 &#xD;&#xA;when LAKOH_TYPE=2 and TZ_TYPE=14 and TT.KVUTZAT_MASTER in (8,12) then 2 &#xD;&#xA;when LAKOH_TYPE=2 and TZ_TYPE=11 and TT.KVUTZAT_MASTER in (8,12) then 3 &#xD;&#xA;when LAKOH_TYPE=2 and TZ_TYPE=1 and TT.KVUTZAT_MASTER in (8,12) then 4 &#xD;&#xA;when LAKOH_TYPE=3 and TZ_TYPE=17 and TT.KVUTZAT_MASTER in (8,12) then 7 &#xD;&#xA;when LAKOH_TYPE=3 and TZ_TYPE=13 and TT.KVUTZAT_MASTER in (8,12) then 8 &#xD;&#xA;when LAKOH_TYPE=3 and TZ_TYPE=19 and TT.KVUTZAT_MASTER in (8,12) then 9 &#xD;&#xA;when LAKOH_TYPE=3 and TZ_TYPE=18 and TT.KVUTZAT_MASTER in (8,12) then 10 &#xD;&#xA;when LAKOH_TYPE=3 and TZ_TYPE=15 and TT.KVUTZAT_MASTER in (8,12) then 11 &#xD;&#xA;when LAKOH_TYPE=1 and TZ_TYPE=1  and TT.KVUTZAT_MASTER in (8,12) then 13&#xD;&#xA;else &#xD;&#xA;13&#xD;&#xA;end BusinessType_OK&#xD;&#xA;--,H.BusinessType as CurrentBusinessType&#xD;&#xA;from A_HALVAOT H,A_KISHR_HALVAOT K,A_LAKOHOT L,A_TAVLAT_TSHUVOT TT&#xD;&#xA;where K.NUMERATOR_TIK=H.NUMERATOR and  K.LOVE_RASHI=1 and K.MISPAR_LAKOAH=L.T_Z&#xD;&#xA;and TT.KOD_TAVLA=16 and TT.KOD_TSHUVA=H.SUG_HALVAA &#xD;&#xA;and H.SUM_HALVAA_SHACH &lt;> 0 and  H.SUG_HALVAA &lt;> 0 &#xD;&#xA;AND TT.KVUTZAT_MASTER &lt;> 99&#xD;&#xA;and H.BusinessType&lt;>case  &#xD;&#xA;when LAKOH_TYPE=2 and TZ_TYPE=5 and TT.KVUTZAT_MASTER in (8,12) then 1 &#xD;&#xA;when LAKOH_TYPE=2 and TZ_TYPE=14 and TT.KVUTZAT_MASTER in (8,12) then 2 &#xD;&#xA;when LAKOH_TYPE=2 and TZ_TYPE=11 and TT.KVUTZAT_MASTER in (8,12) then 3 &#xD;&#xA;when LAKOH_TYPE=2 and TZ_TYPE=1 and TT.KVUTZAT_MASTER in (8,12) then 4 &#xD;&#xA;when LAKOH_TYPE=3 and TZ_TYPE=17 and TT.KVUTZAT_MASTER in (8,12) then 7 &#xD;&#xA;when LAKOH_TYPE=3 and TZ_TYPE=13 and TT.KVUTZAT_MASTER in (8,12) then 8 &#xD;&#xA;when LAKOH_TYPE=3 and TZ_TYPE=19 and TT.KVUTZAT_MASTER in (8,12) then 9 &#xD;&#xA;when LAKOH_TYPE=3 and TZ_TYPE=18 and TT.KVUTZAT_MASTER in (8,12) then 10 &#xD;&#xA;when LAKOH_TYPE=3 and TZ_TYPE=15 and TT.KVUTZAT_MASTER in (8,12) then 11 &#xD;&#xA;when LAKOH_TYPE=1 and TZ_TYPE=1  and TT.KVUTZAT_MASTER in (8,12) then 13&#xD;&#xA;else &#xD;&#xA;13&#xD;&#xA;end"/>
