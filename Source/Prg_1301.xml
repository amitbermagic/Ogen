          <SQL_FORM DB="DB_AGUDA" RESTAB="Memory">
            <SQL_STMT_U val="SELECT H.NUMERATOR&#xD;&#xA;,convert(DATE, H.HALVAA_DATE, 112) AS HALVAA_DATE&#xD;&#xA;,H.SUM_HALVAA_SHACH&#xD;&#xA;,lb.Loan_PayOff_Balance_No_State&#xD;&#xA;,tash.C_Tash&#xD;&#xA;,lb.Loan_PastDue_Balance - lb.Loan_PastDue_Balance_State AS Past_Due_Balance&#xD;&#xA;,TS.TEUR AS STATUS&#xD;&#xA;FROM A_HALVAOT H&#xD;&#xA;JOIN V_Loan_Balance_Now lb ON lb.NUMERATOR = H.NUMERATOR&#xD;&#xA;JOIN A_TAVLAT_TSHUVOT TS on TS.KOD_TAVLA=1 and TS.KOD_TSHUVA =H.STATUS_HALVAA&#xD;&#xA;JOIN (&#xD;&#xA;SELECT T.NUMERATOR_TIK&#xD;&#xA;,count(*) AS C_Tash&#xD;&#xA;FROM A_TASHLUMIM T&#xD;&#xA;WHERE T.TZURAT_TASHLUM &lt;> 10&#xD;&#xA;AND T.TZURAT_TASHLUM &lt;> 20&#xD;&#xA;AND T.STATUS_TASHLUM &lt; 4&#xD;&#xA;and SUG_TASHLUM=0&#xD;&#xA;GROUP BY T.NUMERATOR_TIK&#xD;&#xA;) tash ON tash.NUMERATOR_TIK = H.NUMERATOR&#xD;&#xA;WHERE H.HALVAA_DATE &lt;> '00000000'&#xD;&#xA;AND tash.C_Tash > 0&#xD;&#xA;AND H.SUM_HALVAA_SHACH>0&#xD;&#xA;and H.SUG_HALVAA&lt;>999&#xD;&#xA;and H.STATUS_HALVAA not in (9,10)&#xD;&#xA;order by H.NUMERATOR&#xD;&#xA;"/>
